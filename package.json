{
  "name": "mofacts-automated-authoring",
  "version": "2.0.1",
  "description": "Mofacts Automated Authoring",
  "scripts": {
    "start": "webpack-dev-server",
    "publish": "gh-pages -d deploy",
    "deploy": "webpack -p",
    "node-build": "fable-splitter src -o node-build --commonjs",
    "node-build-with-sourcemaps": "fable-splitter src -o node-build --commonjs -c splitter.config.js",
    "pretest": "fable-splitter tests -o test-build --commonjs",
    "test": "mocha test-build -r jsdom-global/register -t 10000  --require tests/test-setup.js"
  },
  "keywords": [
    "subtitle",
    "speech",
    "alignment",
    "manual"
  ],
  "author": "Andrew M. Olney",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/runtime-corejs2": "^7.0.0",
    "@fortawesome/fontawesome-free": "^5.8.1",
    "bulma": "^0.5.2",
    "diff": "^4.0.2",
    "isomorphic-fetch": "^3.0.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "transliteration": "^2.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^6.3.0",
    "core-js": "^3.0.0",
    "css-loader": "^2.1.1",
    "fable-compiler": "^2.8.4",
    "fable-loader": "^2.1.9",
    "fable-splitter": "^2.1.13",
    "file-loader": "^4.0.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^15.1.0",
    "jsdom-global": "^3.0.2",
    "loglevel": "^1.5.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^7.1.2",
    "node-sass": "^4.14.1",
    "raf-stub": "^2.0.2",
    "remotedev": "^0.2.9",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.26.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
